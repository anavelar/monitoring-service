{
  "handlers": {
    "default": {
      "type": "pipe",
      "command":"tee /dev/null"
    },
    "mailer": {
      "type": "pipe",
      "command": "/usr/local/bundle/bin/handler-mailer.rb",
      "filter": "noise-keepalive"
    },
    "influxdb_udp":{
      "type": "udp",
      "socket": {
        "host": "influxdba",
        "port": 8089
      },
      "mutator": "influxdb_line_protocol"
    }
  }
}
